{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
    "vapi-run": "pip3 install -r requirements.txt && python3 api/vapi.py",
    "ngrok-run": "ngrok http 5001",
    "next-dev": "next dev",
    "dev": "concurrently 'pnpm run next-dev' 'pnpm run flask-dev' 'pnpm run vapi-run' 'pnpm run ngrok-run'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "framer-motion": "^11.11.9",
    "next": "^13.4.3",
    "next-pwa": "^5.6.0",
    "pnpm": "^9.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.5.4",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  }
}
